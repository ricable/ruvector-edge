  ],
  "parameters": [
    {
      "name": "tddCaFrameAlignment",
      "moClass": "EUtranCellTDD",
      "dataType": "enumeration",
      "description": "Frame alignment mode for TDD CA"
    },
    {
      "name": "tddCaUlDlConfig",
      "moClass": "EUtranCellTDD",
      "dataType": "enumeration",
      "description": "UL/DL configuration for TDD CA"
    }
  ],
  "counters": [
    {
      "name": "pmTddCaActivationRate",
      "type": "gauge",
      "description": "TDD CA activation rate"
    },
    {
      "name": "pmTddCaFrameMismatch",
      "type": "counter",
      "description": "Frame alignment mismatch counter"
    }
  ],
  "kpis": [
    {
      "name": "TDD CA Activation Success Rate",
      "formula": "successfulTddCaActivations / totalTddCaAttempts",
      "targetValue": "> 94%",
      "thresholdWarning": "< 89%",
      "thresholdCritical": "< 84%"
    },
    {
      "name": "TDD CA Frame Alignment Rate",
      "formula": "(totalTddCaAttempts - pmTddCaFrameMismatch) / totalTddCaAttempts",
      "targetValue": "> 99%",
      "thresholdWarning": "< 97%",
      "thresholdCritical": "< 95%"
    }
  ],
