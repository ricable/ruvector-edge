{
  "agentId": "agent-ca-004",
  "fajCode": "FAJ 121 0201",
  "cxcCode": "CAE_004",
  "featureName": "Uplink Carrier Aggregation",
  "acronym": "ULCA",
  "category": "Carrier Aggregation",
  "subcategory": "Uplink",
  "accessTechnology": "LTE",
  "specialization": "Aggregation of uplink carriers for improved uplink throughput and coverage",
  "description": "Enables UE to transmit on multiple uplink carriers simultaneously, significantly improving uplink data rates",
  "qLearning": {
    "alpha": 0.1,
    "gamma": 0.9,
    "epsilon": 0.3,
    "epsilonDecay": 0.995,
    "epsilonMin": 0.05,
    "learningRateStrategy": "adaptive",
    "rewardDiscounting": "temporal"
  },
  "stateMachine": {
    "coldStartThreshold": 100,
    "healthWarningThreshold": 0.7,
    "healthCriticalThreshold": 0.5,
    "recoveryThreshold": 0.8,
    "maxConsecutiveFailures": 5,
    "observationWindowMs": 5000,
    "degradedActions": ["prune_q_table", "increase_exploration", "federated_sync"]
  },
  "capabilities": [
    "ParameterConfiguration",
    "Troubleshooting",
    "Optimization",
    "Activation",
    "Deactivation",
    "Comparison",
    "GeneralInfo"
  ],
  "parameters": [
    {
      "name": "ulCaEnabled",
      "moClass": "EUtranCellFDD",
      "dataType": "boolean",
      "description": "Enable/disable uplink carrier aggregation"
    },
    {
      "name": "ulCaSecondaryCarrier",
      "moClass": "EUtranCellFDD",
      "dataType": "enumeration",
      "description": "Configuration for secondary uplink carrier"
    }
  ],
  "counters": [
    {
      "name": "pmUlCaActivationRate",
      "type": "gauge",
      "description": "Rate of UL CA activation attempts"
    },
    {
      "name": "pmUlCaThroughput",
      "type": "gauge",
      "description": "Average uplink throughput with UL CA"
    }
  ],
  "kpis": [
    {
      "name": "UL CA Activation Success Rate",
      "formula": "successfulUlCaActivations / totalUlCaAttempts",
      "targetValue": "> 95%",
      "thresholdWarning": "< 90%",
      "thresholdCritical": "< 85%"
    },
    {
      "name": "UL CA Throughput Gain",
      "formula": "ulCaThroughput / singleCarrierUlThroughput",
      "targetValue": "> 1.6x",
      "thresholdWarning": "< 1.3x",
      "thresholdCritical": "< 1.1x"
    }
  ],
