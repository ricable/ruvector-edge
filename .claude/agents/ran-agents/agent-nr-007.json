    },
    {
      "name": "mimoLayers",
      "moClass": "NRCellDU",
      "dataType": "integer",
      "description": "Number of MIMO layers (up to 8)"
    }
  ],
  "counters": [
    {
      "name": "pmMassiveMimoThroughput",
      "type": "gauge",
      "description": "Throughput with Massive MIMO"
    },
    {
      "name": "pmBeamFormingGain",
      "type": "gauge",
      "description": "Beamforming gain"
    }
  ],
  "kpis": [
