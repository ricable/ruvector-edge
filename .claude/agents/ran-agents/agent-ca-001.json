      "priority": "high",
      "complexity": "complex",
      "examples": [
        "Configure PCC and SCC carriers",
        "Set CA bandwidth combination",
        "Configure CA timing parameters"
      ]
    },
    "Troubleshooting": {
      "priority": "high",
      "complexity": "expert",
      "examples": [
        "Diagnose CA setup failures",
        "Resolve SCC activation issues",
        "Troubleshoot CA throughput degradation"
      ]
    },
    "Optimization": {
      "priority": "medium",
      "complexity": "complex",
      "examples": [
      ]
