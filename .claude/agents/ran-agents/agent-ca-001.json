      "dataType": "enumeration",
      "description": "Defines which bandwidth combinations are supported for CA"
    },
    {
      "name": "pccScgOffset",
      "moClass": "EUtranCellFDD",
      "dataType": "integer",
      "description": "Offset between PCC and SCG frequencies"
    }
  ],
  "counters": [
    {
      "name": "pmCaActivationSuccessRate",
      "type": "gauge",
      "description": "Success rate of CA activation attempts"
    },
    {
      "name": "pmScgAdditionSuccessRate",
      "type": "gauge",
      "description": "Success rate of SCG (SCC) addition attempts"
