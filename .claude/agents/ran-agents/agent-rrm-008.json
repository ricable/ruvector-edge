  "category": "Radio Resource Management",
  "subcategory": "Power Control",
  "accessTechnology": "LTE",
  "specialization": "Open and closed loop uplink power control for interference management",
  "description": "Controls UE transmit power to maintain link quality while minimizing interference",
  "qLearning": {
    "alpha": 0.15,
    "gamma": 0.85,
    "epsilon": 0.25,
    "epsilonDecay": 0.994,
    "epsilonMin": 0.05,
    "learningRateStrategy": "adaptive",
    "rewardDiscounting": "temporal"
  },
  "stateMachine": {
    "coldStartThreshold": 100,
    "healthWarningThreshold": 0.7,
    "healthCriticalThreshold": 0.5,
    "recoveryThreshold": 0.8,
    "maxConsecutiveFailures": 5,
    "observationWindowMs": 5000,
    "degradedActions": ["prune_q_table", "increase_exploration", "federated_sync"]
  },
  "capabilities": [
    "ParameterConfiguration",
    "Troubleshooting",
    "Optimization",
    "Activation",
    "Deactivation",
    "Comparison",
    "GeneralInfo"
  ],
  "parameters": [
    {
      "name": "p0NominalPusch",
      "moClass": "EUtranCellFDD",
      "dataType": "integer",
      "description": "Nominal UE transmit power for PUSCH"
    },
    {
      "name": "alphaUplink",
      "moClass": "EUtranCellFDD",
      "dataType": "enumeration",
      "description": "Pathloss compensation factor"
    }
  ],
  "counters": [
    {
      "name": "pmUlPowerHeadroom",
      "type": "gauge",
      "description": "Average uplink power headroom"
    },
    {
      "name": "pmUlInterference",
      "type": "gauge",
      "description": "Uplink interference level"
    }
  ],
  "kpis": [
    {
      "name": "UL Power Headroom",
      "formula": "pmUlPowerHeadroom",
      "targetValue": "> 10dB",
      "thresholdWarning": "< 5dB",
      "thresholdCritical": "< 0dB"
    },
    {
      "name": "UL Interference Level",
      "formula": "pmUlInterference",
      "targetValue": "< -110dBm",
      "thresholdWarning": "> -105dBm",
      "thresholdCritical": "> -100dBm"
    }
  ],
