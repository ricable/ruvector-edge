{
  "agentId": "agent-rrm-006",
  "fajCode": "FAJ 120 0201",
  "cxcCode": "RRM_006",
  "featureName": "Congestion Control",
  "acronym": "CC",
  "category": "Radio Resource Management",
  "subcategory": "Congestion",
  "accessTechnology": "LTE",
  "specialization": "Detection and mitigation of network congestion through dynamic resource management",
  "description": "Monitors cell load and implements congestion mitigation strategies to maintain QoS",
  "qLearning": {
    "alpha": 0.15,
    "gamma": 0.85,
    "epsilon": 0.25,
    "epsilonDecay": 0.994,
    "epsilonMin": 0.05,
    "learningRateStrategy": "adaptive",
    "rewardDiscounting": "temporal"
  },
  "stateMachine": {
    "coldStartThreshold": 100,
    "healthWarningThreshold": 0.7,
    "healthCriticalThreshold": 0.5,
    "recoveryThreshold": 0.8,
    "maxConsecutiveFailures": 5,
    "observationWindowMs": 5000,
    "degradedActions": ["prune_q_table", "increase_exploration", "federated_sync"]
  },
  "capabilities": [
    "ParameterConfiguration",
    "Troubleshooting",
    "Optimization",
    "Activation",
    "Deactivation",
    "Comparison",
    "GeneralInfo"
  ],
  "parameters": [
    {
      "name": "congestionThreshold",
      "moClass": "EUtranCellFDD",
      "dataType": "integer",
      "description": "Utilization threshold for congestion detection"
    },
    {
      "name": "congestionMitigationStrategy",
      "moClass": "EUtranCellFDD",
      "dataType": "enumeration",
      "description": "Strategy for congestion mitigation (drop, degrade, redirect)"
    }
  ],
  "counters": [
    {
      "name": "pmCongestionEvents",
      "type": "counter",
      "description": "Number of congestion events detected"
    },
    {
      "name": "pmCongestionDuration",
      "type": "gauge",
      "description": "Average duration of congestion events"
    }
  ],
  "kpis": [
    {
      "name": "Congestion Event Rate",
      "formula": "pmCongestionEvents / totalTime",
      "targetValue": "< 5/hour",
      "thresholdWarning": "> 10/hour",
      "thresholdCritical": "> 20/hour"
    },
    {
      "name": "Congestion Recovery Time",
      "formula": "pmCongestionDuration",
      "targetValue": "< 30s",
      "thresholdWarning": "> 60s",
      "thresholdCritical": "> 120s"
    }
  ],
  "dependencies": [
    "FAJ 120 0101"
  ],
  "conflicts": [],
  "relatedFeatures": [
    "FAJ 120 0102",
    "FAJ 120 0103"
  ],
  "releases": {
    "introduced": "17A",
    "deprecated": null,
    "supportedIn": ["17A", "17B", "18A", "18B", "19A", "19B", "20A", "20B", "21A", "21B", "22A", "22B", "23A", "23B"]
  },
  "moClasses": [
    "EUtranCellFDD",
    "CongestionControl"
  ],
  "documents": [
    "Congestion Control Configuration Guide",
    "Congestion Mitigation Strategies"
  ],
