{
  "agentId": "agent-nr-001",
  "fajCode": "FAJ 150 0101",
  "cxcCode": "NR5G_001",
  "featureName": "NR NSA (Non-Standalone)",
  "acronym": "NSA",
  "category": "NR/5G",
  "subcategory": "NSA",
  "accessTechnology": "NR",
  "specialization": "5G NR non-standalone deployment with LTE anchor",
  "description": "Enables 5G NR deployment with LTE as anchor for control plane, leveraging existing LTE infrastructure",
  "qLearning": {
    "alpha": 0.12,
    "gamma": 0.88,
    "epsilon": 0.28,
    "epsilonDecay": 0.994,
    "epsilonMin": 0.05,
    "learningRateStrategy": "adaptive",
    "rewardDiscounting": "temporal"
  },
  "stateMachine": {
    "coldStartThreshold": 100,
    "healthWarningThreshold": 0.7,
    "healthCriticalThreshold": 0.5,
    "recoveryThreshold": 0.8,
    "maxConsecutiveFailures": 5,
    "observationWindowMs": 5000,
    "degradedActions": ["prune_q_table", "increase_exploration", "federated_sync"]
  },
  "capabilities": [
    "ParameterConfiguration",
    "Troubleshooting",
    "Optimization",
    "Activation",
    "Deactivation",
    "Comparison",
    "GeneralInfo"
  ],
  "parameters": [
    {
      "name": "nsaMode",
      "moClass": "NRCellDU",
      "dataType": "enumeration",
      "description": "NSA deployment mode (EN-DC, NE-DC, NGEN-DC)"
    },
    {
      "name": "enDcRelease",
      "moClass": "NRCellDU",
      "dataType": "enumeration",
      "description": "EN-DC release version"
    }
  ],
  "counters": [
    {
      "name": "pmNsaSetupSuccessRate",
      "type": "gauge",
      "description": "NSA setup success rate"
    },
    {
      "name": "pmNrDlThroughput",
      "type": "gauge",
      "description": "NR downlink throughput"
    }
  ],
  "kpis": [
    {
      "name": "NSA Setup Success Rate",
      "formula": "pmNsaSetupSuccessRate",
      "targetValue": "> 95%",
      "thresholdWarning": "< 90%",
      "thresholdCritical": "< 85%"
    },
    {
      "name": "NR Throughput Gain vs LTE",
      "formula": "pmNrDlThroughput / lteDlThroughput",
      "targetValue": "> 3x",
      "thresholdWarning": "< 2x",
      "thresholdCritical": "< 1.5x"
    }
  ],
  "dependencies": [
    "FAJ 100 0101"
  ],
  "conflicts": [],
  "relatedFeatures": [
