    },
    {
      "name": "nrCcCount",
      "moClass": "NRCellDU",
      "dataType": "integer",
      "description": "Number of NR component carriers"
    }
  ],
  "counters": [
    {
      "name": "pmNrCaSetupRate",
      "type": "gauge",
      "description": "NR CA setup rate"
    },
    {
      "name": "pmNrCaThroughput",
      "type": "gauge",
      "description": "NR CA throughput"
    }
