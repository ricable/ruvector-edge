  "featureName": "QoS-Based Scheduling",
  "acronym": "QOSSCHED",
  "category": "Radio Resource Management",
  "subcategory": "QoS",
  "accessTechnology": "LTE",
  "specialization": "Differentiated scheduling based on QoS class and bearer priority",
  "description": "Implements QoS-aware scheduling to meet latency and throughput requirements for different services",
  "qLearning": {
    "alpha": 0.15,
    "gamma": 0.85,
    "epsilon": 0.25,
    "epsilonDecay": 0.994,
    "epsilonMin": 0.05,
    "learningRateStrategy": "adaptive",
    "rewardDiscounting": "temporal"
  },
  "stateMachine": {
    "coldStartThreshold": 100,
    "healthWarningThreshold": 0.7,
    "healthCriticalThreshold": 0.5,
    "recoveryThreshold": 0.8,
    "maxConsecutiveFailures": 5,
    "observationWindowMs": 5000,
    "degradedActions": ["prune_q_table", "increase_exploration", "federated_sync"]
  },
  "capabilities": [
    "ParameterConfiguration",
    "Troubleshooting",
    "Optimization",
    "Activation",
    "Deactivation",
    "Comparison",
    "GeneralInfo"
  ],
  "parameters": [
    {
      "name": "qosSchedulingEnabled",
      "moClass": "EUtranCellFDD",
      "dataType": "boolean",
      "description": "Enable QoS-based scheduling"
    },
    {
      "name": "qosWeightGbR",
      "moClass": "EUtranCellFDD",
      "dataType": "integer",
      "description": "Scheduling weight for GBR bearers"
    }
  ],
  "counters": [
    {
      "name": "pmQoSatisfactionRate",
      "type": "gauge",
      "description": "Rate of QoS requirements satisfaction"
    },
    {
      "name": "pmGbRThroughput",
      "type": "gauge",
      "description": "Average throughput for GBR bearers"
    }
  ],
  "kpis": [
    {
      "name": "QoS Satisfaction Rate",
      "formula": "pmQoSatisfactionRate",
      "targetValue": "> 95%",
      "thresholdWarning": "< 90%",
      "thresholdCritical": "< 85%"
    },
    {
      "name": "GBR Throughput Achievement",
      "formula": "pmGbRThroughput / gbrTarget",
      "targetValue": "> 98%",
      "thresholdWarning": "< 95%",
      "thresholdCritical": "< 90%"
    }
  ],
