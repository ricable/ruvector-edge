  "parameters": [
    {
      "name": "psEnabled",
      "moClass": "EUtranCellFDD",
      "dataType": "boolean",
      "description": "Enable semi-persistent scheduling"
    },
    {
      "name": "psInterval",
      "moClass": "EUtranCellFDD",
      "dataType": "integer",
      "description": "SPS interval in milliseconds"
    }
  ],
  "counters": [
    {
      "name": "pmSpsGrantRate",
      "type": "gauge",
      "description": "Rate of SPS grants"
    },
    {
      "name": "pmSpsSignalingReduction",
      "type": "gauge",
      "description": "Signaling reduction achieved with SPS"
    }
  ],
  "kpis": [
    {
      "name": "SPS Grant Rate",
      "formula": "pmSpsGrantRate",
      "targetValue": "> 80%",
