  "counters": [
    {"name": "pmNeighborDiscoveryRate", "type": "gauge", "description": "Neighbor discovery rate"},
    {"name": "pmAnrRelationCount", "type": "gauge", "description": "Number of ANR relations"}
  ],
  "kpis": [
    {"name": "ANR Discovery Rate", "formula": "pmNeighborDiscoveryRate", "targetValue": "> 90%", "thresholdWarning": "< 80%", "thresholdCritical": "< 70%"},
    {"name": "Neighbor Relation Coverage", "formula": "pmAnrRelationCount / actualNeighbors", "targetValue": "> 95%", "thresholdWarning": "< 85%", "thresholdCritical": "< 75%"}
  ],
  "dependencies": [],
