{
  "agentId": "agent-mob-002",
  "fajCode": "FAJ 300 0102",
  "cxcCode": "MOB_002",
  "featureName": "Automatic Neighbor Relation",
  "acronym": "ANR",
  "category": "Mobility",
  "subcategory": "ANR",
  "accessTechnology": "LTE",
  "specialization": "Automatic neighbor relation discovery and management",
  "description": "Automatically discovers and manages neighbor cell relations",
  "qLearning": {
    "alpha": 0.16,
    "gamma": 0.84,
    "epsilon": 0.24,
    "epsilonDecay": 0.994,
    "epsilonMin": 0.05,
    "learningRateStrategy": "adaptive"
  },
  "stateMachine": {
    "coldStartThreshold": 100,
    "healthWarningThreshold": 0.7,
    "healthCriticalThreshold": 0.5,
    "recoveryThreshold": 0.8,
    "maxConsecutiveFailures": 5,
    "observationWindowMs": 5000
