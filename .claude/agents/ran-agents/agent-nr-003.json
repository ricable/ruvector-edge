{
  "agentId": "agent-nr-003",
  "fajCode": "FAJ 150 0201",
  "cxcCode": "NR5G_003",
  "featureName": "Dual Connectivity (EN-DC)",
  "acronym": "EN-DC",
  "category": "NR/5G",
  "subcategory": "Dual Connectivity",
  "accessTechnology": "NR",
  "specialization": "E-UTRA-NR Dual Connectivity with LTE and NR simultaneous connection",
  "description": "Enables UEs to connect to both LTE and NR simultaneously for improved throughput and mobility",
  "qLearning": {
    "alpha": 0.12,
    "gamma": 0.88,
    "epsilon": 0.28,
    "epsilonDecay": 0.994,
    "epsilonMin": 0.05,
    "learningRateStrategy": "adaptive",
    "rewardDiscounting": "temporal"
  },
  "stateMachine": {
    "coldStartThreshold": 100,
    "healthWarningThreshold": 0.7,
    "healthCriticalThreshold": 0.5,
    "recoveryThreshold": 0.8,
    "maxConsecutiveFailures": 5,
    "observationWindowMs": 5000,
    "degradedActions": ["prune_q_table", "increase_exploration", "federated_sync"]
  },
  "capabilities": [
    "ParameterConfiguration",
    "Troubleshooting",
    "Optimization",
    "Activation",
    "Deactivation",
    "Comparison",
    "GeneralInfo"
  ],
  "parameters": [
    {
      "name": "enDcEnabled",
      "moClass": "ENDCFunction",
      "dataType": "boolean",
      "description": "Enable EN-DC dual connectivity"
    },
    {
      "name": "enDcBearerSetup",
      "moClass": "ENDCFunction",
      "dataType": "enumeration",
      "description": "Bearer setup mode for EN-DC"
    }
  ],
  "counters": [
    {
      "name": "pmEnDcSetupRate",
      "type": "gauge",
      "description": "EN-DC setup rate"
    },
    {
      "name": "pmEnDcThroughput",
      "type": "gauge",
      "description": "Combined LTE-NR throughput"
    }
  ],
  "kpis": [
    {
      "name": "EN-DC Setup Success Rate",
      "formula": "pmEnDcSetupRate",
      "targetValue": "> 94%",
      "thresholdWarning": "< 89%",
      "thresholdCritical": "< 84%"
    },
    {
      "name": "EN-DC Throughput Gain",
      "formula": "pmEnDcThroughput / lteOnlyThroughput",
      "targetValue": "> 4x",
      "thresholdWarning": "< 3x",
      "thresholdCritical": "< 2x"
    }
  ],
  "dependencies": [
