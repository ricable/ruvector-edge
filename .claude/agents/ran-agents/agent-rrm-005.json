    },
    {
      "name": "ifLbThreshold",
      "moClass": "EUtranCellFDD",
      "dataType": "integer",
      "description": "Load threshold for inter-frequency load balancing"
    },
    {
      "name": "ifLbOffset",
      "moClass": "EUtranCellFDD",
      "dataType": "integer",
      "description": "Measurement offset for target frequency"
