    {
      "name": "CA Interference Reduction",
      "formula": "interferenceWithoutScrambling / interferenceWithScrambling",
      "targetValue": "> 1.2x",
      "thresholdWarning": "< 1.1x",
      "thresholdCritical": "< 1.05x"
    }
  ],
