  "parameters": [
    {
      "name": "caScramblingEnabled",
      "moClass": "CarrierAggregation",
      "dataType": "boolean",
      "description": "Enable CA supplementary scrambling"
    },
    {
      "name": "caScramblingCode",
      "moClass": "CarrierAggregation",
      "dataType": "integer",
      "description": "Scrambling code for CA channels"
    }
  ],
  "counters": [
    {
      "name": "pmCaScramblingBlER",
      "type": "gauge",
      "description": "BLER with CA scrambling enabled"
    },
    {
      "name": "pmCaInterferenceIndex",
      "type": "gauge",
      "description": "Interference index for CA users with scrambling"
    }
  ],
  "kpis": [
    {
      "name": "CA Scrambling BLER Improvement",
      "formula": "blerWithoutScrambling - blerWithScrambling",
      "targetValue": "> 2%",
      "thresholdWarning": "< 1%",
      "thresholdCritical": "< 0%"
